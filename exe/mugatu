#!/usr/bin/env ruby

if File.exist?(File.expand_path("../../.git", __FILE__))
  lib = File.expand_path("../../lib", __FILE__)
  $LOAD_PATH.unshift(lib) unless $LOAD_PATH.include?(lib)
end

require "mugatu"
require "mugatu/cli"

runner = Mugatu::Cli::Run.parse(ARGV)
# Mugatu::Cli::Dispatcher.start(ARGV)
